{
  "name": "ormojo-elasticsearch",
  "version": "0.0.2",
  "description": "Elasticsearch driver for ormojo.",
  "main": "src/index.coffee",
  "scripts": {
    "clean": "rm -rf js",
    "build": "mkdir -p js && coffee -o js src/*.coffee",
    "build_doc": "node_modules/.bin/codo src/",
    "prepublish": "npm run clean && npm run build",
    "test": "node_modules/.bin/mocha --recursive --compilers coffee:coffee-script/register",
    "coverage": "node_modules/.bin/mocha --recursive --compilers coffee:coffee-script/register --require coffee-coverage/register-istanbul",
    "coverageReport": "node_modules/.bin/istanbul report"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wcjohnson/ormojo-elasticsearch.git"
  },
  "keywords": [
    "ormojo",
    "backend",
    "nosql",
    "elasticsearch"
  ],
  "author": "William C. Johnson <wcjohnson@oigroup.net> (https://github.com/wcjohnson)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wcjohnson/ormojo-elasticsearch/issues"
  },
  "homepage": "https://github.com/wcjohnson/ormojo-elasticsearch",
  "devDependencies": {
    "blackbird-promises": "0.0.1",
    "chai": "^3.5.0",
    "coffee-coverage": "^1.0.1",
    "coffee-script": "^1.10.0",
    "elasticsearch": "^11.0.1",
    "esq": "^1.0.0",
    "istanbul": "^0.4.5",
    "mocha": "^2.4.5"
  },
  "dependencies": {
    "lodash": "^3.10.1",
    "deep-diff": "^0.3.4"
  },
  "peerDependencies": {}
}
